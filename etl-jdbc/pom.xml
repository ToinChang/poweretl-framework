<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
 <modelVersion>4.0.0</modelVersion>



 <parent>
  <groupId>com.kollect.etl</groupId>
  <artifactId>etl-parent</artifactId>
  <version>0.0.1-SNAPSHOT</version>
 </parent>
 <artifactId>etl-jdbc</artifactId>
 <name>etl-jdbc</name>
 <url>http://maven.apache.org</url>





 <properties>
  <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
 </properties>

 <dependencies>
  <dependency>
   <groupId>junit</groupId>
   <artifactId>junit</artifactId>
   <version>4.12</version>
   <scope>test</scope>
  </dependency>

  <dependency>
   <groupId>ie.ucd</groupId>
   <artifactId>murmur</artifactId>
   <version>0.1.0</version>
  </dependency>

  <dependency>
   <groupId>com.kollect.etl</groupId>
   <artifactId>etl-utils</artifactId>
   <version>${project.version}</version>
  </dependency>

  <dependency>
   <groupId>com.kollect.etl</groupId>
   <artifactId>etl-dataaccess</artifactId>
   <version>${project.version}</version>
  </dependency>

 </dependencies>



 <build>
  <testResources>
   <testResource>
    <directory>/home/joshua/martian/ptrworkspace/etl_yyc_impl/config</directory>
   </testResource>
  </testResources>
  <resources>
   <resource>
    <directory>/home/joshua/martian/ptrworkspace/etl_yyc_impl/config</directory>
   </resource>
  </resources>
  <pluginManagement>
   <plugins>
    <!-- maven compiler plugin -->
    <plugin>
     <groupId>org.apache.maven.plugins</groupId>
     <artifactId>maven-compiler-plugin</artifactId>
     <version>3.7.0</version>
     <configuration>
      <source>1.8</source>
      <target>1.8</target>
      <verbose>false</verbose>
     </configuration>
    </plugin>
    <!-- maven execution plugin -->
    <plugin>
     <groupId>org.codehaus.mojo</groupId>
     <artifactId>exec-maven-plugin</artifactId>
     <version>1.6.0</version>
     <executions>
      <execution>
       <goals>
        <goal>java</goal>
       </goals>
      </execution>
     </executions>
     <configuration>
      <mainClass></mainClass>
      <addResourcesToClasspath>true</addResourcesToClasspath>
      <arguments>
       <argument></argument>
       <argument></argument>
      </arguments>
     </configuration>
    </plugin>
   </plugins>
  </pluginManagement>
 </build>
</project>
